<Window x:Name="___No_Name_" x:Class="PackageShippingGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PackageShippingGUI"
        mc:Ignorable="d"
        Title="Package Shipping GUI" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="packageGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,48,0,0" IsReadOnly="True"/>
        <Button x:Name="AddPackage" Content="Add New Package" HorizontalAlignment="Left" Height="23" Margin="28,10,0,0" VerticalAlignment="Top" Width="106" Click="AddPackage_Click"/>
    </Grid>
</Window>
